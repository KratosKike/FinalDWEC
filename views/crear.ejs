      <div class="container">
        <h1>Crear nueva Baraja</h1>
        <form action="/barajas" class="my-2" method="POST">
            <input type="text" placeholder="Nombre Baraja" class="form-control my-2" name="nombre">
            <input type="text" placeholder="Juego" class="form-control my-2" name="juego">
            <!--<input type="text" placeholder="Lista" class="form-control my-2" name="lista">-->
            <div class="form-floating">
              <textarea class="form-control" placeholder="Leave a comment here" id="floatingTextarea2" style="height: 100px" name="lista">
              </textarea>
              <label for="floatingTextarea2"></label>
            </div>
            <button type="submit" class="btn btn-dark btn-block">
                Agregar
            </button>
        </form>
    </div>
